# AWS ECR Deployment Configuration
# Copy this file to .env and fill in your actual values

# Your 12-digit AWS Account ID (REQUIRED)
# Get it by running: aws sts get-caller-identity --query Account --output text
AWS_ACCOUNT_ID=123456789012

# AWS region where you want to create the ECR repository
AWS_REGION=us-east-1

# ECR repository name (will be created if it doesn't exist)
ECR_REPOSITORY=ai-ml-intern-website

# Docker image tag
IMAGE_TAG=latest

# Whether to clean up local images after push (true/false)
CLEANUP_LOCAL=false

# Frontend Environment Variables
# Copy this file to .env and update values as needed

# Backend API URL
REACT_APP_API_URL=http://localhost:5000/api

# Other environment variables can be added here as needed
